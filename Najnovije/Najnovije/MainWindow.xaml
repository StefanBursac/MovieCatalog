<Window x:Class="Najnovije.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Najnovije"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>

        <Button x:Name="button" Content="Button" Width="170" Height="85" Margin="30,190,317,44" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="95" Margin="30,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="65" TextChanged="textBox_TextChanged"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="73" Margin="130,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95" RenderTransformOrigin="-0.842,1.027"/>
        <TextBox x:Name="textBox2" Text="{Binding Name}" HorizontalAlignment="Left" Height="43" Margin="165,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245"/>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" Margin="290,190,0,0" VerticalAlignment="Top" Width="170" Height="75" Click="button1_Click"/>

    </Grid>
</Window>
